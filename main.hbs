                        <div class="col-sm-12 mb-3">
                            <div class="card-deck">
                                <div class="card iteration-0">
                                    <div class="card-header text-center  iteration-0">
                                        <a data-toggle="collapse" href="#" data-target="#rule-collapse-{{../type}}-{{code}}"
                                            aria-expanded="false" aria-controls="collapse" id="#rule-{{../type}}-{{code}}"
                                            class="collapsed text-dark z-block">

                                            {{name}}<i class="float-lg-right fa fa-chevron-down"
                                                style="padding-top: 5px;"></i>
                                        </a>
                                    </div>
                                    <div id="rule-collapse-{{../type}}-{{code}}" class="collapse"
                                        aria-labelledby="rule-{{../type}}-{{code}}">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-sm-12 mb-3">
                                                    <div class="card-deck">
                                                        <div class="card border-info" style="width: 50rem;">
                                                            <div class="card-body">
                                                                <h5
                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                    Description</h5>
                                                                <div class="dyn-height">
                                                                    <code class="renderMarkdown">{{description}}</code>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-12 mb-3">
                                                    <div class="card-deck">
                                                        <div class="card border-info" style="width: 50rem;">
                                                            {{#if passed}}
                                                                            <div class="card-body">
                                                                                <h5
                                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                                    Rule Status</h5>
                                                                                <div>
                                                                                    <div class="progress"
                                                                                        style="height: 40px;">
                                                                                        <div class="progress-bar bg-success"
                                                                                            style="width: 100%"
                                                                                            role="progressbar">
                                                                                            <h5
                                                                                                style="padding-top:5px;">
                                                                                                <strong>Passed</strong>
                                                                                            </h5>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
