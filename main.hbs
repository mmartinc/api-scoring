        <script>
            $("#filterInput").on("input paste", function () {
                var value = $(this).val();
                $("#iterationList li").filter(function () {
                    $("#showOnlyFailures").data("clickCount") ? $("#showOnlyFailures").click() : null;
                    $(this).toggle($(this).text().indexOf(value) > -1)
