<body class="theme-light">
        <div class="card">
            <div class="card-header">
                <ul class="nav nav-pills mb-3 nav-justified" id="pills-tab" role="tablist">
                    <li class="nav-item bg-summary" data-toggle="tooltip" title="Click to view the Summary">
                        <a class="nav-link text-dark" id="pills-summary-tab" data-toggle="pill" href="#pills-summary"
                            role="tab" aria-controls="pills-summary" aria-selected="true">Summary <span
                                class="badge badge-light">49</span></a>
                    </li>

                    {{#each errors}}
                    <li class="nav-item bg-{{type}}" data-toggle="tooltip" title="Click to view the {{type}} failed rules">
                        <a class="nav-link text-white" id="pills-{{type}}-tab" data-toggle="pill" href="#pills-{{type}}"
                            role="tab" aria-controls="pills-{{type}}" aria-selected="false"><span class="text-{{type}}">{{title}} </span><span
                                class="badge badge-light">{{fails.length}}</span></a>
                    </li>
                    {{/each}}

                </ul>
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade show active" id="pills-summary" role="tabcard"
                        aria-labelledby="pills-summary-tab">
                        <div class="row">
                            <div class="col-md-9 col-lg-12 main">
                                <h5 class="text-center">{{date}}</h5>
                                <div class="row">
                                    <div class="col-lg-3 col-md-6">
                                        {{#if case-5}}
                                        <div class="card text-white card-dark">
                                            <div class="card-body bg-unacceptable">
                                                <div class="rotate">
                                                    <i class="fas fa-dizzy fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Scoring</h6>
                                                <h4 class="text-uppercase">{{result}}</h4>
                                                <h1 class="display-1">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}
                                        {{#if case-4}}
                                        <div class="card text-white card-dark">
                                            <div class="card-body bg-danger">
                                                <div class="rotate">
                                                    <i class="far fa-frown fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Scoring</h6>
                                                <h4 class="text-uppercase">{{result}}</h4>
                                                <h1 class="display-1">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}
                                        {{#if case-3}}
                                        <div class="card text-white card-warning">
                                            <div class="card-body bg-warning">
                                                <div class="rotate">
                                                    <i class="far fa-meh fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Scoring</h6>
                                                <h4 class="text-uppercase">{{result}}</h4>
                                                <h1 class="display-1">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}
                                        {{#if case-2}}
                                        <div class="card text-white card-info">
                                            <div class="card-body bg-info">
                                                <div class="rotate">
                                                    <i class="far fa-smile fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Scoring</h6>
                                                <h4 class="text-uppercase">{{result}}</h4>
                                                <h1 class="display-1">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}
                                        {{#if case-1}}
                                        <div class="card text-white card-primary">
                                            <div class="card-body bg-hint">
                                                <div class="rotate">
                                                    <i class="far fa-grin fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase text-dark">Scoring</h6>
                                                <h4 class="text-uppercase text-dark">{{result}}</h4>
                                                <h1 class="display-1 text-dark">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}
                                        {{#if case-0}}
                                        <div class="card text-white card-success">
                                            <div class="card-body bg-success">
                                                <div class="rotate">
                                                    <i class="far fa-laugh fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Scoring</h6>
                                                <h4 class="text-uppercase">{{result}}</h4>
                                                <h1 class="display-1">{{scoring}}</h1>
                                            </div>
                                        </div>
                                        {{/if}}

                                    </div>
                                    <div class="col-lg-3 col-md-6">
                                        <div class="card text-white card-success">
                                            <div class="card-body bg-summary">
                                                <div class="rotate">
                                                    
                                                    <i class="fa fa-list fa-4x"></i>
                                                </div>
                                                <h6 class="text-uppercase text-dark">Total Rules</h6>
                                                <h4 class="text-uppercase text-dark"><br></h4>
                                                <h1 class="display-1 text-dark">{{total-rules}}</h1>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-6">
                                        <div class="card text-white card-danger">
                                            <div class="card-body bg-success">
                                                <div class="rotate">
                                                    <i class="fas fa-check-circle fa-4x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Passed Rules</h6>
                                                <h4 class="text-uppercase"><br></h4>
                                                <h1 class="display-1">{{total-passed-rules}}</h1>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-6">
                                        <div class="card text-white card-info">
                                            <div class="card-body bg-danger">
                                                <div class="rotate">
                                                    <i class="fa fa-times-circle fa-5x"></i>
                                                </div>
                                                <h6 class="text-uppercase">Failed Rules</h6>
                                                <h4 class="text-uppercase"><br></h4>
                                                <h1 class="display-1">{{total-failed-rules}}</h1>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col">
                                        <div class="row">
                                            <div class="col-sm-12 mb-3">
                                                <div class="card border-info">
                                                    <div class="card-body">
                                                        <h5
                                                            class="card-title text-uppercase text-white text-center bg-info">
                                                            File Information</h5>
                                                        <span><i class="fas fa-file-code"></i></span><strong>
                                                            API:</strong>{{api-name}}<br>


                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 mb-3">
                                                <div class="card border-info">
                                                    <div class="card-body">
                                                        <h5
                                                            class="card-title text-uppercase text-white text-center bg-info">
                                                            API Description</h5>
                                                        <div class="dyn-height">
                                                            <code class="renderMarkdown">{{api-description}}</code>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 mb-3">
                                                <div class="card border-info">
                                                    <div class="card-body">
                                                        <h5
                                                            class="card-title text-uppercase text-white text-center bg-info">
                                                            Timings</h5>
                                                        <span><i class="fas fa-stopwatch"></i></span><strong> Total run
                                                            duration:</strong> {{time}}s<br>
                                                        <span><i class="fas fa-stopwatch"></i></span><strong> Average
                                                            time per rule:</strong> {{time-rule}}ms<br>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 mb-3">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-bordered">
                                                        <thead class="thead-inverse">
                                                            <tr class="text-center">
                                                                <th class="text-uppercase">Summary Item</th>
                                                                <th class="text-uppercase">Total</th>
                                                                <th class="text-uppercase">Failed</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            {{#each errors}}
                                                            <tr>
                                                                <td>{{title}}</td>
                                                                <td class="text-center">{{total}}</td>
                                                                <td class="text-center">{{fails.length}}
                                                                </td>
                                                            </tr>
                                                            {{/each}}
                                                            <tr class="">
                                                                <td>Total Rules</td>
                                                                <td class="text-center">{{total-rules}}</td>
                                                                <td class="text-center">{{total-failed-rules}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="btn-group float-right" role="group" aria-label="Button Group">
                            
                            <button id="openAll" class="btn btn-outline-success btn-sm float-right"
                                style="text-align: center; width: 140px;">Expand Folders</button>
                            <button id="openAllRules" class="btn btn-outline-success btn-sm float-right"
                                style="text-align: center; width: 140px;">Expand Rules</button>
                        </div>

                        <div class="text-uppercase" id="execution-menu">
                            <h5>Full ruleset summary</h5>
                        </div>
                        {{#each ruleset-summary}}
                        <div class="tab-content" id="execution-data">
                            <div class="alert alert-dark text-uppercase text-center iteration-0">
                                <a data-toggle="collapse" href="#" data-target="#folder-collapse-{{package}}"
                                    aria-expanded="false" aria-controls="collapse" id="folder-{{package}}"
                                    class="collapsed text-dark z-block">
                                    <i class="float-left resultsInfoPass" style="color: #c24a3f; padding-top: 4px;"></i>
                                    {{package}}<i class="float-lg-right fa fa-chevron-down"
                                        style="padding-top: 5px;"></i>
                                </a>
                            </div>
                            <div id="folder-collapse-{{package}}" class="collapse" aria-labelledby="folder-{{package}}">
                                <div id="folder-{{package}}" class="card-deck iteration-0">
                                    <div class="row iteration-0">
                                        {{#each rules}}
                                        <div class="col-sm-12 mb-3">
                                            <div class="card-deck">
                                                <div class="card iteration-0">
                                                    <div class="card-header  text-center  iteration-0">
                                                        <a data-toggle="collapse" href="#"
                                                            data-target="#rule-collapse-{{code}}" aria-expanded="false"
                                                            aria-controls="collapse" id="rule-{{code}}"
                                                            class="collapsed text-dark z-block">
                                                            {{#if passed}}
                                                            <i class="fas fa-check-circle float-left resultsInfoPass"
                                                                style="color: #009b5c; padding-top: 4px;"></i>
                                                            {{else}}
                                                            <i class="card-title fas fa-times-circle float-left resultsInfoPass"
                                                                style="color: #cd212a; padding-top: 4px;"></i>

                                                            {{/if}}
                                                            {{name}}<i class="float-lg-right fa fa-chevron-down"
                                                                style="padding-top: 5px;"></i>
                                                        </a>
                                                    </div>
                                                    <div id="rule-collapse-{{code}}" class="collapse"
                                                        aria-labelledby="rule-{{code}}">
                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="col-sm-12 mb-3">
                                                                    <div class="card-deck">
                                                                        <div class="card border-info"
                                                                            style="width: 50rem;">
                                                                            <div class="card-body">
                                                                                <h5
                                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                                    Description</h5>
                                                                                <div class="dyn-height">
                                                                                    <code
                                                                                        class="renderMarkdown">{{description}}</code>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-sm-12 mb-3">
                                                                    <div class="card-deck">
                                                                        <div class="card border-info"
                                                                            style="width: 50rem;">
                                                                            {{#if passed}}
                                                                            <div class="card-body">
                                                                                <h5
                                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                                    Rule Status</h5>
                                                                                <div>
                                                                                    <div class="progress"
                                                                                        style="height: 40px;">
                                                                                        <div class="progress-bar bg-success"
                                                                                            style="width: 100%"
                                                                                            role="progressbar">
                                                                                            <h5
                                                                                                style="padding-top:5px;">
                                                                                                <strong>Passed</strong>
                                                                                            </h5>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>


                                                                            {{else}}
                                                                            <div class="card-body">
                                                                                <h5
                                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                                    Rule Status</h5>
                                                                                <div>
                                                                                    <div class="progress"
                                                                                        style="height: 40px;">
                                                                                        <div class="progress-bar bg-danger"
                                                                                            style="width: 100%"
                                                                                            role="progressbar">
                                                                                            <h5
                                                                                                style="padding-top:5px;">
                                                                                                <strong>Failed</strong>
                                                                                            </h5>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <hr>

                                                                                <div>
                                                                                    <h5
                                                                                        class="card-title text-uppercase text-white text-center bg-info">
                                                                                        Fail ranges</h5>
                                                                                    {{#each range}}
                                                                                    <span><i
                                                                                            class="fas fa-exclamation-circle"></i></span><strong>
                                                                                        Lines:</strong> <span
                                                                                        class="float-right badge-outline badge badge-success">
                                                                                        {{lines}}</span><br>
                                                                                    {{/each}}
                                                                                </div>
                                                                            </div>
                                                                            {{/if}}



                                                                        </div>
                                                                        <div class="card border-info"
                                                                            style="width: 50rem;">
                                                                            <div class="card-body">
                                                                                <h5
                                                                                    class="card-title text-uppercase text-white text-center bg-info">
                                                                                    Rule Information</h5>
                                                                                <span><i
                                                                                        class="fas fa-lightbulb"></i></span><strong>
                                                                                    Severity:</strong> <span
                                                                                    class="float-right badge-outline badge badge-success">{{severity}}</span><br>
                                                                                <span><i
                                                                                        class="fas fa-balance-scale"></i></span><strong>
                                                                                    Criticality:</strong> <span
                                                                                    class="float-right badge-outline badge badge-success">
                                                                                    {{criticaly}}</span><br>
                                                                                <span><i
                                                                                        class="fas fa-weight"></i></span><strong>
                                                                                    Score:</strong> <span
                                                                                    class="float-right badge-outline badge badge-success">
                                                                                    {{score}}</span><br>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>
                                </div>
                            </div>



                        </div>
                        {{/each}}
                    </div>
                    {{#each errors}}
                    <div class="tab-pane fade" id="pills-{{type}}" role="tabcard" aria-labelledby="pills-{{type}}-tab">
                        {{#if fails.length}}
                        <div class="btn-group float-right" role="group" aria-label="Button Group">
                            <button id="openAllFailed{{type}}" class="btn btn-outline-success btn-sm float-right"
                                style="text-align: center; width: 185px;">Expand All Failed Rules</button>
                        </div>
                        <br>
                        <br>

                        <div class="alert alert-{{type}} text-uppercase text-center">
                            <h4 class="text-{{type}}">Showing {{fails.length}} {{type}}s</h4>
                        </div>
